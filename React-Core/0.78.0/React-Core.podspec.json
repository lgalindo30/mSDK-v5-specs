{
  "name": "React-Core",
  "version": "0.78.0",
  "summary": "The core of React Native.",
  "homepage": "https://reactnative.dev/",
  "license": "MIT",
  "authors": "Meta Platforms, Inc. and its affiliates",
  "platforms": {
    "ios": "15.1"
  },
  "source": {
    "git": "https://github.com/facebook/react-native.git",
    "tag": "v0.78.0"
  },
  "resource_bundles": {
    "React-Core_privacy": "packages/react-native/React/Resources/PrivacyInfo.xcprivacy"
  },
  "compiler_flags": "-DFOLLY_NO_CONFIG -DFOLLY_MOBILE=1 -DFOLLY_USE_LIBCPP=1 -DFOLLY_CFG_NO_COROUTINES=1 -DFOLLY_HAVE_CLOCK_GETTIME=1 -Wno-comma -Wno-shorten-64-to-32 -Wno-documentation -DUSE_HERMES=1",
  "header_dir": "React",
  "weak_frameworks": "JavaScriptCore",
  "pod_target_xcconfig": {
    "HEADER_SEARCH_PATHS": "\"$(PODS_TARGET_SRCROOT)/ReactCommon\" \"$(PODS_TARGET_SRCROOT)/packages/react-native\" \"$(PODS_ROOT)/boost\" \"$(PODS_ROOT)/DoubleConversion\" \"$(PODS_ROOT)/fast_float/include\" \"$(PODS_ROOT)/fmt/include\" \"$(PODS_ROOT)/RCT-Folly\" \"${PODS_ROOT}/Headers/Public/FlipperKit\" \"$(PODS_ROOT)/Headers/Public/ReactCommon\" \"$(PODS_ROOT)/Headers/Public/React\" \"$(PODS_ROOT)/Headers/Public/React-hermes\" \"$(PODS_ROOT)/Headers/Public/hermes-engine\" \"$(PODS_ROOT)/React-hermes\" \"$(PODS_ROOT)/React-hermes/reacthermes\" \"$(PODS_ROOT)/Headers/Private/Yoga\" \"$(PODS_ROOT)/Headers/Public/React-Fabric\" \"$(PODS_ROOT)/Headers/Public/React-Fabric/react/renderer\" \"$(PODS_ROOT)/Headers/Public/React-jsc\" \"$(PODS_ROOT)/Headers/Private/React-jsc\" \"${PODS_CONFIGURATION_BUILD_DIR}/React-hermes/React_hermes.framework/Headers\" \"${PODS_CONFIGURATION_BUILD_DIR}/React-jsinspector/jsinspector_modern.framework/Headers\" \"${PODS_CONFIGURATION_BUILD_DIR}/RCTDeprecation/RCTDeprecation.framework/Headers\" \"${PODS_CONFIGURATION_BUILD_DIR}/React-jsc/React_jsc.framework/Headers\"",
    "DEFINES_MODULE": "YES",
    "GCC_PREPROCESSOR_DEFINITIONS": "RCT_METRO_PORT=${RCT_METRO_PORT}",
    "CLANG_CXX_LANGUAGE_STANDARD": "c++20",
    "FRAMEWORK_SEARCH_PATHS": "\"$(PODS_CONFIGURATION_BUILD_DIR)/React-hermes\""
  },
  "user_target_xcconfig": {
    "HEADER_SEARCH_PATHS": "\"$(PODS_ROOT)/Headers/Private/React-Core\""
  },
  "default_subspecs": "Default",
  "dependencies": {
    "RCT-Folly": [
      "2024.11.18.00"
    ],
    "React-cxxreact": [],
    "React-perflogger": [],
    "React-jsi": [],
    "React-jsiexecutor": [],
    "React-utils": [],
    "React-featureflags": [],
    "SocketRocket": [
      "0.7.1"
    ],
    "React-runtimescheduler": [],
    "Yoga": [],
    "glog": [],
    "React-jsinspector": [],
    "RCTDeprecation": [],
    "React-hermes": [],
    "hermes-engine": []
  },
  "subspecs": [
    {
      "name": "Default",
      "source_files": "packages/react-native/React/**/*.{c,h,m,mm,S,cpp}",
      "exclude_files": [
        "packages/react-native/React/CoreModules/**/*",
        "packages/react-native/React/DevSupport/**/*",
        "packages/react-native/React/Fabric/**/*",
        "packages/react-native/React/FBReactNativeSpec/**/*",
        "packages/react-native/React/Tests/**/*",
        "packages/react-native/React/Inspector/**/*",
        "packages/react-native/React/CxxBridge/JSCExecutorFactory.{h,mm}"
      ],
      "private_header_files": "packages/react-native/React/Cxx*/*.h"
    },
    {
      "name": "DevSupport",
      "source_files": [
        "packages/react-native/React/DevSupport/*.{h,mm,m}",
        "packages/react-native/React/Inspector/*.{h,mm,m}"
      ],
      "dependencies": {
        "React-Core/Default": [
          "0.78.0"
        ],
        "React-Core/RCTWebSocket": [
          "0.78.0"
        ]
      },
      "private_header_files": "packages/react-native/React/Inspector/RCTCxx*.h"
    },
    {
      "name": "RCTWebSocket",
      "source_files": "packages/react-native/Libraries/WebSocket/*.{h,m}",
      "dependencies": {
        "React-Core/Default": [
          "0.78.0"
        ]
      }
    },
    {
      "name": "CoreModulesHeaders",
      "source_files": "packages/react-native/React/CoreModules/**/*.h",
      "dependencies": {
        "React-Core/Default": []
      }
    },
    {
      "name": "RCTActionSheetHeaders",
      "source_files": "packages/react-native/Libraries/ActionSheetIOS/*.h",
      "dependencies": {
        "React-Core/Default": []
      }
    },
    {
      "name": "RCTAnimationHeaders",
      "source_files": "packages/react-native/Libraries/NativeAnimation/{Drivers/*,Nodes/*,*}.{h}",
      "dependencies": {
        "React-Core/Default": []
      }
    },
    {
      "name": "RCTBlobHeaders",
      "source_files": "packages/react-native/Libraries/Blob/{RCTBlobManager,RCTFileReaderModule}.h",
      "dependencies": {
        "React-Core/Default": []
      }
    },
    {
      "name": "RCTImageHeaders",
      "source_files": "packages/react-native/Libraries/Image/*.h",
      "dependencies": {
        "React-Core/Default": []
      }
    },
    {
      "name": "RCTLinkingHeaders",
      "source_files": "packages/react-native/Libraries/LinkingIOS/*.h",
      "dependencies": {
        "React-Core/Default": []
      }
    },
    {
      "name": "RCTNetworkHeaders",
      "source_files": "packages/react-native/Libraries/Network/*.h",
      "dependencies": {
        "React-Core/Default": []
      }
    },
    {
      "name": "RCTPushNotificationHeaders",
      "source_files": "packages/react-native/Libraries/PushNotificationIOS/*.h",
      "dependencies": {
        "React-Core/Default": []
      }
    },
    {
      "name": "RCTSettingsHeaders",
      "source_files": "packages/react-native/Libraries/Settings/*.h",
      "dependencies": {
        "React-Core/Default": []
      }
    },
    {
      "name": "RCTTextHeaders",
      "source_files": "packages/react-native/Libraries/Text/**/*.h",
      "dependencies": {
        "React-Core/Default": []
      }
    },
    {
      "name": "RCTVibrationHeaders",
      "source_files": "packages/react-native/Libraries/Vibration/*.h",
      "dependencies": {
        "React-Core/Default": []
      }
    }
  ],
  "script_phases": [
    {
      "name": "Copy React public headers",
      "script": "FABRIC_SRC=\"${PODS_ROOT}/React-RCTFabric/packages/react-native/React/Fabric/Mounting/ComponentViews\"\nPUBLIC_REACT=\"${PODS_ROOT}/Headers/Public/React\"\nmkdir -p \"$PUBLIC_REACT\"\nif [ -f \"$FABRIC_SRC/RCTFabricComponentsPlugins.h\" ]; then cp -f \"$FABRIC_SRC/RCTFabricComponentsPlugins.h\" \"$PUBLIC_REACT/RCTFabricComponentsPlugins.h\"; fi\nif [ -f \"$FABRIC_SRC/View/RCTViewComponentView.h\" ]; then cp -f \"$FABRIC_SRC/View/RCTViewComponentView.h\" \"$PUBLIC_REACT/RCTViewComponentView.h\"; fi\nFOLLY_SRC=\"${PODS_ROOT}/React-Core/packages/react-native/React/CxxUtils\"\nif [ -f \"$FOLLY_SRC/RCTFollyConvert.h\" ]; then cp -f \"$FOLLY_SRC/RCTFollyConvert.h\" \"$PUBLIC_REACT/RCTFollyConvert.h\"; fi\nFB_SPEC_SRC=\"${PODS_ROOT}/React-Core/packages/react-native/React/FBReactNativeSpec/FBReactNativeSpec\"\nFB_SPEC_DST=\"${PODS_ROOT}/Headers/Public/FBReactNativeSpec\"\nmkdir -p \"$FB_SPEC_DST\"\nif [ -d \"$FB_SPEC_SRC\" ]; then cp -f \"$FB_SPEC_SRC\"/*.h \"$FB_SPEC_DST\"/; fi\nTRACE_SRC=\"${PODS_ROOT}/React-jsinspectortracing/packages/react-native/ReactCommon/jsinspector-modern/tracing\"\nTRACE_DST=\"${PODS_ROOT}/Headers/Public/jsinspector-modern/tracing\"\nmkdir -p \"$TRACE_DST\"\nif [ -d \"$TRACE_SRC\" ]; then cp -f \"$TRACE_SRC\"/*.h \"$TRACE_DST\"/; fi",
      "execution_position": "before_compile"
    }
  ]
}